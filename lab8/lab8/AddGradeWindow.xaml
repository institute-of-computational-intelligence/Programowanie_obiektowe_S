<Window x:Class="lab8.AddGradeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab8"
        mc:Ignorable="d"
        Title="AddGradeWindow" Height="200" Width="340">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="115,22,0,0" TextWrapping="Wrap" Text="Przedmiot" VerticalAlignment="Top" Height="23" Width="82"/>
        <TextBlock HorizontalAlignment="Left" Margin="115,65,0,0" TextWrapping="Wrap" Text="Ocena" VerticalAlignment="Top" Height="22" Width="82"/>
        <TextBox x:Name="tb" HorizontalAlignment="Left" Height="23" Margin="202,22,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <ComboBox x:Name="cb" HorizontalAlignment="Left" Margin="202,65,0,0" VerticalAlignment="Top" Width="120" IsReadOnly="True"/>
        <Button x:Name="AddButton" Content="Add" HorizontalAlignment="Left" Margin="239,117,0,0" VerticalAlignment="Top" Width="75" Click="AddButton_Click"/>
        <Button x:Name="AbortButton" Content="Cancel" HorizontalAlignment="Left" Margin="122,117,0,0" VerticalAlignment="Top" Width="75" Click="AbortButton_Click"/>

    </Grid>
</Window>

<Window x:Class="lab8.StudentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab8"
        mc:Ignorable="d"
        Title="StudentEdit" Height="220" Width="400">
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="150,20,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="220" Name="tbName"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="150,50,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="220" Name="tbSurname"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="150,80,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="220" Name="tbIndex"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="150,110,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="220" Name="tbFaculty"/>
        <TextBlock HorizontalAlignment="Left" Margin="90,25,0,0" TextWrapping="Wrap" Text="Imię" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="90,55,0,0" TextWrapping="Wrap" Text="Nazwisko" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="90,85,0,0" TextWrapping="Wrap" Text="Nr indeksu" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="90,115,0,0" TextWrapping="Wrap" Text="Wydział" VerticalAlignment="Top"/>
        <Button Content="Ok" HorizontalAlignment="Left" Margin="295,143,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
